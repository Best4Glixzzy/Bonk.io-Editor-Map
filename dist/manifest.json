{
  "manifest_version": 2,
  "name": "kklee",
  "version": "1.0.0",
  "description": "Extends the functionality of the Bonk.io map editor.",
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "*://bonk.io/*"
  ],
  "background": {
    "scripts": [
      "./background.js"
    ]
  },
  "web_accessible_resources": [
    "inject.js"
  ]
}
