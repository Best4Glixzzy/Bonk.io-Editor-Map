{
  "manifest_version": 2,
  "name": "kklee",
  "version": "0.1",
  "description": "Extends the functionality of the Bonk.io map editor.",
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "*://bonk.io/*"
  ],
  "background": {
    "scripts": [
      "./background.js"
    ]
  },
  "web_accessible_resources": [
    "inject.js"
  ],
  "icons": {
    "64": "icons/icon.png"
  }
}
